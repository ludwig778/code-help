{
  "plugins": [
    "testing-library",
    "jest",
    "jest-dom"
  ],
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:testing-library/react",
    "plugin:jest/recommended",
    "plugin:jest-dom/recommended"
  ],
  "rules": {
    "import/no-anonymous-default-export": ["error", {
      "allowArrowFunction": true
    }]
  }
}

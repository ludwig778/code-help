{
  "name": "react-base-app",
  "version": "1.0.0",
  "description": "React boiler plate",
  "scripts": {
    "sh": "bash",
    "lint": "eslint src",
    "prettier": "prettier --write src/",
    "dev": "webpack-dev-server",
    "test": "jest",
    "test:cov": "jest --coverage=true",
    "test:debug": "jest --verbose --watchAll",
    "test:types": "tsc --project tsconfig.json --noEmit",
    "test:types:debug": "tsc --project tsconfig.json --noEmit --watch",
    "build": "webpack build",
    "build:postcss": "postcss src/styles/main.css -o dist/output.css",
    "build:prod": "webpack build --env production",
    "check": "npm run test; npm run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-jest-dom": "^3.9.4",
    "eslint-plugin-testing-library": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.4.5",
    "msw": "^0.36.5",
    "npm-check-updates": "^16.7.4",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.15",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
